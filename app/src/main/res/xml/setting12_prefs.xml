<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:settings="http://schemas.android.com/apk/res/com.android.settings"
    android:title="修改密码" >
    <EditTextPreference
        android:key="username"
        android:title="请输入你当前的密码"
        android:inputType="textPassword">
    </EditTextPreference>
    <EditTextPreference
        android:key="password1"
        android:title="请输入你的新密码"
        android:inputType="textPassword">
    </EditTextPreference>
    <EditTextPreference
        android:key="password2"
        android:title="再次输入你的新密码"
        android:inputType="textPassword">
    </EditTextPreference>
    <Preference
        android:key="submit"
        android:title="提交">
    </Preference>



</PreferenceScreen>